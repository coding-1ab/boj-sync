{
	"printWidth": 60,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "lf",
	"vueIndentScriptAndStyle": true,
	"overrides": [
		{
			"files": ".swcrc",
			"options": {
				"parser": "json"
			}
		}
	]
}
